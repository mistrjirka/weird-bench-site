{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["benchmark_name", "timestamp", "results"],
  "properties": {
    "benchmark_name": {
      "type": "string",
      "const": "7zip"
    },
    "timestamp": {
      "type": "number"
    },
    "results": {
      "type": "object",
      "required": ["meta", "build", "runs"],
      "properties": {
        "meta": {
          "type": "object",
          "required": ["benchmark_name", "host", "platform", "timestamp"],
          "properties": {
            "benchmark_name": {
              "type": "string",
              "const": "7zip"
            },
            "host": {
              "type": "string"
            },
            "platform": {
              "type": "string"
            },
            "timestamp": {
              "type": "number"
            }
          }
        },
        "build": {
          "type": "object",
          "required": ["sevenzip_command", "build_time_seconds"],
          "properties": {
            "sevenzip_command": {
              "type": "string"
            },
            "build_time_seconds": {
              "type": "number"
            },
            "notes": {
              "type": "string"
            }
          }
        },
        "runs": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": ["success", "threads", "elapsed_seconds", "archive_size_bytes", "compression_ratio", "compression_speed_mb_s", "raw_output"],
            "properties": {
              "success": {
                "type": "boolean"
              },
              "threads": {
                "type": "integer",
                "minimum": 1
              },
              "elapsed_seconds": {
                "type": "number",
                "minimum": 0
              },
              "archive_size_bytes": {
                "type": "integer",
                "minimum": 0
              },
              "compression_ratio": {
                "type": "number",
                "minimum": 0
              },
              "compression_speed_mb_s": {
                "type": "number",
                "minimum": 0
              },
              "raw_output": {
                "type": "string"
              },
              "speedup": {
                "type": "number",
                "minimum": 0
              },
              "thread_efficiency_percent": {
                "type": "number",
                "minimum": 0
              }
            }
          }
        },
        "test_data_size_mb": {
          "type": "integer",
          "minimum": 0
        }
      }
    }
  }
}